<!-- start product Area -->
	
	<section class="owl-carousel active-product-area section_gap">
		<!-- single product slide -->
		{{#each result}}
		<div class="single-product-slider">
			<div class="container">
				<div class="row justify-content-center">
					<div class="col-lg-6 text-center">
						<div class="section-title">
							<h1>Gian h√†ng {{this.cate}}</h1>
							
						</div>
					</div>
				</div>
				<div class="row">
					{{#each this.data}}
					<!-- single product -->
					<div class="col-lg-3 col-md-6">
						<div class="single-product">
							<a href="/singleproduct/{{this._slug}}">
								<img  width="200" height="200" src="{{this._img}}">
							</a>
							<div class="product-details">
								<h6>{{this._nameproduct}}</h6>
								<div class="price">
									<h6>${{this._price}}</h6>
									
								</div>
								
							</div>
						</div>
					</div>
					{{/each}}
					
				</div>
			</div>
		</div>
		{{/each}}
		
		
	</section>
	<!-- end product Area -->
	

	

	